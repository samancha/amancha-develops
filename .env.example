# Firebase Configuration
# Get these values from Firebase Console > Project Settings > Your apps > Web

# Copy this file to .env and fill in your actual Firebase credentials
# .env is gitignored and won't be committed to the repository

VITE_FIREBASE_API_KEY=YOUR_API_KEY_HERE
VITE_FIREBASE_PROJECT_ID=YOUR_PROJECT_ID_HERE
VITE_FIREBASE_AUTH_DOMAIN=your-project.firebaseapp.com
VITE_FIREBASE_STORAGE_BUCKET=your-project.appspot.com
VITE_FIREBASE_MESSAGING_SENDER_ID=YOUR_MESSAGING_SENDER_ID_HERE
VITE_FIREBASE_APP_ID=1:YOUR_APP_ID_HERE:web:YOUR_WEB_ID_HERE
